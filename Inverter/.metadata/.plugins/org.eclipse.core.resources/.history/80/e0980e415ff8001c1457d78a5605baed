#ifndef PI_CONTROLLER_H_
#define PI_CONTROLLER_H_



//loat Tcyc = 1.0 /5000.0; //Tcyc sample correspond to control update rate to PI controler for I

//################ PI
// PID Controller declaration for Id and Iq component
typedef struct{
	float P,I;
	float MaxLimit, MinLimit;
	float Output;
	float Deviation_old, Deviation_old2;
	float Output_z;
}PI_param;


float PI_Controller(float SetPoint,float ActValue,PID_param *param);


#endif /* PI_CONTROLLER_H_ */
